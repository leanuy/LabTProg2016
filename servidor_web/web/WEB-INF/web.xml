<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>VerPerfil</servlet-name>
        <servlet-class>servlets.VerPerfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrarme</servlet-name>
        <servlet-class>servlets.Registrarme</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargarDatos</servlet-name>
        <servlet-class>servlets.CargarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerAlbum</servlet-name>
        <servlet-class>servlets.VerAlbum</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registrarme</servlet-name>
        <url-pattern>/Registrarme</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerPerfil</servlet-name>
        <url-pattern>/VerPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargarDatos</servlet-name>
        <url-pattern>/CargarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerAlbum</servlet-name>
        <url-pattern>/VerAlbum</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
