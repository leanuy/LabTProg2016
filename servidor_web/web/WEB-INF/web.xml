<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Inicio</servlet-name>
        <servlet-class>servlets.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>iniciar-Sesion</servlet-name>
        <servlet-class>servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerPerfil</servlet-name>
        <servlet-class>servlets.VerPerfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrarme</servlet-name>
        <servlet-class>servlets.Registrarme</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CargarDatos</servlet-name>
        <servlet-class>servlets.CargarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerAlbum</servlet-name>
        <servlet-class>servlets.VerAlbum</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MiPerfil</servlet-name>
        <servlet-class>servlets.MiPerfil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerListaParticular</servlet-name>
        <servlet-class>servlets.VerListaParticular</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeguirUsuario</servlet-name>
        <servlet-class>servlets.SeguirUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublicarLista</servlet-name>
        <servlet-class>servlets.PublicarLista</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Artistas</servlet-name>
        <servlet-class>servlets.Artistas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerGenero</servlet-name>
        <servlet-class>servlets.VerGenero</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Inicio</servlet-name>
        <url-pattern>/inicio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>iniciar-Sesion</servlet-name>
        <url-pattern>/iniciar-sesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/cerrar-sesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrarme</servlet-name>
        <url-pattern>/Registrarme</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerPerfil</servlet-name>
        <url-pattern>/VerPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CargarDatos</servlet-name>
        <url-pattern>/CargarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerAlbum</servlet-name>
        <url-pattern>/VerAlbum</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeguirUsuario</servlet-name>
        <url-pattern>/SeguirUsuario</url-pattern>
    </servlet-mapping>
    <!--    <error-page>
        <error-code>404</error-code>
        <location>/WEB-INF/errors/404.jsp</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/WEB-INF/errors/500.jsp</location>
    </error-page>-->
    <servlet-mapping>
        <servlet-name>MiPerfil</servlet-name>
        <url-pattern>/MiPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerListaParticular</servlet-name>
        <url-pattern>/VerListaParticular</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublicarLista</servlet-name>
        <url-pattern>/publicarlista</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Artistas</servlet-name>
        <url-pattern>/Artistas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerGenero</servlet-name>
        <url-pattern>/VerGenero</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
